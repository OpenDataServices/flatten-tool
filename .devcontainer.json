{
	"name": "Flatten Tool Dev Container",
	"build": {
		"dockerfile": "Dockerfile.devcontainer",
		"context": "."
	},
	"customizations": {
		"vscode": {
			"extensions":["ms-python.python"]
		}
	},
	"remoteUser": "dev",
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
	"workspaceFolder": "/workspace"
}
